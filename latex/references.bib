@inproceedings{tridexel_intersection,
	title={Bridging the gap between CSG and Brep via a triple ray representation},
	author={Benouamer, Mohand Ourabah and Michelucci, Dominique},
	booktitle={Proceedings of the fourth ACM symposium on Solid modeling and applications},
	pages={68--79},
	year={1997},
	organization={ACM},
	annotation={As popular representations of volumes with free-form surfaces like z-maps, depth images or ray representations (dexels) do not work well together with boundary representations (Brep), which are often represented by triangle meshes, this paper proposes a triple ray representation (tri-dexel) to combine free-form surfaces with triangle-based meshes for intersection tests. }
}

@inproceedings{mesh_intersection,
	title={Seamless Intersection Between Triangle Meshes},
	author={Rosen, David},
	booktitle={Proceedings: Senior Conference on Computational Geometry},
	pages={1--8},
	year={2008},
	annotation={This paper proposes a method for enhancing the visual appeal of mesh intersections in video games and similar applications. To soften sharp intersection lines between two objects (e.g. rocks on a beach), geometry can be morphed around the intersection of both meshes and textured differently (using texture bands). The intersecting itself is computed using triangle-triangle intersection tests and constrained Delaunay triangulations of the cut triangles using the intersection segments on the triangle as constraints. }
}

@article{bpa,
	title={The ball-pivoting algorithm for surface reconstruction},
	author={Bernardini, Fausto and Mittleman, Joshua and Rushmeier, Holly and Silva, Cl{\'a}udio and Taubin, Gabriel},
	journal={Visualization and Computer Graphics, IEEE Transactions on},
	volume={5},
	number={4},
	pages={349--359},
	year={1999},
	publisher={IEEE},
	annotation={This paper presents the ball-pivoting algorithm (BPA) for reconstructing triangle meshes from point clouds. According to the paper, the BPA's principle is easy to understand and the algorithm excels in memory and runtime efficiency. The paper describes all steps required to implement the algorithm as well as advantages and shortcomings. Furthermore, runtime benchmarks and test scene results are included. }
}

@article{cutter_workpiece_engagement,
	title={Triangle-mesh based cutter-workpiece engagement extraction for general milling processes},
	author={Gong, Xun},
	year={2013},
	publisher={University of British Columbia},
	annotation={Master thesis describing how cutter-workpiece engagement can be extracted from a given cutter and workpiece geometry as well as a chain of cutter transformations over time. Cutter geometry and transformation paths are used to generate the volume swept by the cutter (swept volume) by reducing the cutter mesh to a point cloud which is than interpolated along the cutter's path using the cutter's chain of transformations. The resulting point cloud is re-triangulated using the ball pivoting algorithm and subtracted from the workpiece mesh. By increasing the cutter's size by a small amount, an intersection test can be performed against the cut workpiece to find the engagement area of the cutter. This area can than be used for calculating forces occuring during the simulated machining process. }
}

@inproceedings{extended_marching_cubes,
	title={Feature sensitive surface extraction from volume data},
	author={Kobbelt, Leif and Botsch, Mario and Schwanecke, Ulrich and Seidel, Hans-Peter},
	booktitle={Proceedings of the 28th annual conference on Computer graphics and interactive techniques},
	pages={57--66},
	year={2001},
	organization={ACM},
	annotation={Based on the marching cubes algorithm, this paper provides a strategy for improving features extracted from a given volumetric data set (a regular grid containing triangles, points, implicit surface functions or scalar distance fields). The proposed extended marching cubes algorithm employs a post-processing step which detects features inside a cell using e.g. normal vectors of the contained points. Additional vertices may then be generated to improve the reconstructed surface, especially concerning features like sharp edges. The discussed algorithm is implemented in OpenVDB.}
}

@incollection{enlight,
	title={New Scalable SIMD-Based Ray Caster Implementation for Virtual Machining},
	author={Leutgeb, Alexander and Welsch, Torsten and Hava, Michael},
	booktitle={Parallel Processing and Applied Mathematics},
	pages={317--326},
	year={2014},
	publisher={Springer},
	annotation={A paper from the author's co-workers on the subject of Enlight. It discusses an optimized ray casting approach for visualizing subtractive manufacturing. The presented implementation is directly based on the regular grid data structure which will be used as primary input for the surface reconstruction tackled by this thesis. Beside grid traversal, intersection counting and optimizations, acceleration by surface cell evaluation (SCE) and triangle elimination is presented. The latter two have direct implications for the reconstruction process. }
}

