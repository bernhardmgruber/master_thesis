@book{sdf_surface_reconstruction,
	title={Surface reconstruction from unorganized points},
	author={Hoppe, Hugues and DeRose, Tony and Duchamp, Tom and McDonald, John and Stuetzle, Werner},
	volume={26},
	number={2},
	year={1992},
	publisher={ACM},
	annotation={Demonstrates an algorithm for creating a triangulation approximating a given point cloud, where each point is near or on the surface. The algorithm does not need normal vectors for each point as it takes them from interpolated tangent planes. The surface is described as a signed distance function, estimating the distance of each sample point to the actual surface. This function is then sampled again by a marching cubes variant which emits the triangles of the surface. }
}

@inproceedings{crust,
	title={A new Voronoi-based surface reconstruction algorithm},
	author={Amenta, Nina and Bern, Marshall and Kamvysselis, Manolis},
	booktitle={Proceedings of the 25th annual conference on Computer graphics and interactive techniques},
	pages={415--421},
	year={1998},
	organization={ACM},
	annotation={Discusses surface reconstruction from a point cloud using 3D voronoid diagrams. For a set of sample points with sufficient quality, guarantees can be made about the topological correctness and convergence towards the original surface. The input point cloud can and should be adaptive with higher resoluations in feature-rich areas. The resulting triangulation is Delaunay.}
}

